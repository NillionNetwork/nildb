{
  "id": "",
  "type": "owned",
  "name": "wallet.schema.json",
  "schema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "array",
    "uniqueItems": true,
    "items": {
      "type": "object",
      "properties": {
        "_id": {
          "type": "string",
          "format": "uuid"
        },
        "wallet": {
          "type": "string",
          "description": "Unique wallet identifier"
        },
        "country": {
          "type": "string",
          "description": "String code representing the user's country"
        },
        "age": {
          "type": "integer",
          "description": "User's age"
        }
      },
      "required": ["_id", "wallet", "country", "age"]
    }
  }
}
